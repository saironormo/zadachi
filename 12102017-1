#include <iostream>
using namespace std;

int main()
{ // Упрощения, которые достигаются аналитически: f = 0, иначе а не сохранилась бы, b+g по этой же причине меньше десяти минус с, чтобы не было перехода через десяток
d = 0 или 9 (второй случай — с переходом через десяток, следовательно r больше нуля), чтобы сохранилась пятёрка.
е = d, потому что если d перешла через десяток, то и е придётся через него перейти для сохранения с
d = e = 0, потому что в случае девятки ABC59 + 0G99R = AMC5K можно упростить до вида 59+990 = 49, а это неправда

for (int r = 0; r < 10; r++)
for (int c = 0; c < 10; c++)
for (int b = 0; b < 10; b++)
for (int g = 0; b + g < 10 - c; g++)
for (int a(0); a < 10; a++)
{cout << a << b << c << "50" << endl;
cout << "0" << g << "00" << r << endl;
cout << a << b + g << c << "5" << r << endl;
cout << endl;}

return 0;
}
