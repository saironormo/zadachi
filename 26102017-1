
#include <iostream>
using namespace std;

int main ()
{int n;
cin >> n;
int *arra = new int [n];
int *arrb = new int [n];
for (int i = 0; i < n; i++)
{ cin >> arra[i] >> arrb[i];}
int temp, tempa;
for (int i = 0; i < n; i++) 
{
for (int j = 0; j < n - 1; j++) {
if (arrb[j] > arrb[j + 1]) {
temp = arrb[j];
arrb[j] = arrb[j + 1];
arrb[j + 1] = temp;
tempa = arra[j];
arra[j] = arra[j + 1];
arra[j + 1] = tempa;
}
}
}


temp = arrb[0];
cout << arra[0] << arrb[0] << endl;
for (int i = 1; i < n; i++)
{if (arra[i] >= temp)
{temp = arrb[i];
cout << arra[i] << arrb[i] << endl;}
}


return 0;

}
